---
import Layout from "../layouts/Layout.astro";

import Hero from "../components/Hero.astro";
import Features from "../components/Features.astro";
import Features2 from "../components/Features2.astro";
import Insel from "../components/Insel.astro";

let title = "Astro Portfolio W52s #9";
let description = "Astro Portfolio W52s #9";
const permalink = Astro.site.href;
let canonicalURL = Astro.request.canonicalURL;
---

<Layout {title} {description} {permalink} current="index" {canonicalURL}>
  <Hero />
  <Features />
  <Insel />
  <Features2 />
</Layout>
